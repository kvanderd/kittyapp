%h2="Welcome #{@user.username}"
-@user.kitties.each do |kitty|
	.whole_kitty
		.kitty_object
			.thumb_image
				=image_tag kitty.image_url(:thumb).to_s
			.kitty_info
				=kitty.link
				=link_to kitty.description, kitty_path(kitty.id)
				%p="By: #{kitty.user.username}"
				%p="created: #{kitty.time_ago.to_s} minutes ago"
				%p="#{kitty.comments.count} comments"
			.aggregate_vote
				="Aggregate Votes:#{kitty.aggregate}"
			